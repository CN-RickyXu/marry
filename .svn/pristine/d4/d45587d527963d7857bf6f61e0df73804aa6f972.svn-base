// partyRecordQuery.js
var app = getApp()
Page({

  /**
   * 页面的初始数据
   */
  data: {
    key:''
  },

  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {
    var that = this
    that.getPayRecordList()
  },
  // 下载通知公告数据
  getPayRecordList: function () {
    var that = this
    wx.request({
      url: 'https://party.mtm2000.cn/party/mobile/mobileIn.do',
      method: 'GET',
      header: {
        method: 'GET_PAY_RECORD',
      },
      data: {
        userId: app.globalData.user.id,
        currentPages:1
      },
      success: function (res) {
        console.log("payRecord_res", res)
        if (200 == res.statusCode) {
          if (res.data.resultList.length >= 1) {
            console.log("list")
            //更新数据
            that.setData({
              list: res.data.resultList
            })
          } else {
            that.setData({
              list: []
            })
          }
          
        }
      },
    })
  },

  /**
   * 生命周期函数--监听页面初次渲染完成
   */
  onReady: function () {
  
  },

  /**
   * 生命周期函数--监听页面显示
   */
  onShow: function () {
  
  },

  /**
   * 生命周期函数--监听页面隐藏
   */
  onHide: function () {
  
  },

  /**
   * 生命周期函数--监听页面卸载
   */
  onUnload: function () {
  
  },

  /**
   * 页面相关事件处理函数--监听用户下拉动作
   */
  onPullDownRefresh: function () {
  
  },

  /**
   * 页面上拉触底事件的处理函数
   */
  onReachBottom: function () {
  
  },

  /**
   * 用户点击右上角分享
   */
  onShareAppMessage: function () {
  
  }
})