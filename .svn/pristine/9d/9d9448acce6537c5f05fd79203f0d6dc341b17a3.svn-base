    <view class="music-control">
      <view>
        <view class="knowledge_base_top">
          <text class="knowledge_base_content_title">缴费日期 {{}}</text>
        </view>
        <!--细线  -->
        <view class="line"> </view>
        <view class="base_info_view">
          <text class="info_text">微信名称</text>
          <text class="text_value">{{"kwok"}}</text>
        </view>
        <!--用户名  -->
        <view class="base_info_view">
          <text class="info_text">用户名</text>
          <text class="text_value">{{account}}</text>
        </view>
      </view>
    </view>
  </view>
</scroll-view>