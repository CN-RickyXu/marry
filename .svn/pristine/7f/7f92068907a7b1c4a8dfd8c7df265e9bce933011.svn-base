// comment.js
var app = getApp()
var api = require('../../../api/api.js')
var commentInput
Page({

  /**
   * 页面的初始数据
   */
  data: {

  },
  // 下载评论net
  showCommentNet: function () {
    var that = this
    wx.request({
      url: api.mobileIn,
      method: 'GET',
      header: {
        method: 'GET_MY_PRAISED_LIST',
      },
      data: {
        userId: app.globalData.user.id
      },
      success: function (res) {
        console.log("my_res", res)
        that.setData({
          praise_list: res.data.FeelingsCommentList
        })
      },
    })
  },

  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {
    console.log("onLoad", 'onLoad')
    var that = this

    that.showCommentNet()

  },

  /**
   * 生命周期函数--监听页面初次渲染完成
   */
  onReady: function () {

  },

  /**
   * 生命周期函数--监听页面显示
   */
  onShow: function () {

  },

  /**
   * 生命周期函数--监听页面隐藏
   */
  onHide: function () {

  },

  /**
   * 生命周期函数--监听页面卸载
   */
  onUnload: function () {

  },

  /**
   * 页面相关事件处理函数--监听用户下拉动作
   */
  onPullDownRefresh: function () {

  },

  /**
   * 页面上拉触底事件的处理函数
   */
  onReachBottom: function () {

