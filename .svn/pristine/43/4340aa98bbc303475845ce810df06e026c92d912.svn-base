<!--addArchives.wxml-->
<view class="view">
<view class="top_view"></view>
      <textarea class="input_title" placeholder="请输入标题.." bindinput="TitleInput1" />
            <textarea  class="input_content"   placeholder="请输入内容.." bindinput="ContentInput1" />
 <view class="is_open_view">
 <image class="open_img" src="../../../image/open_img.png"></image>
 <text class="open_text">是否公开</text>
  <switch bindchange="btnOpen" class="close_state_img" type="switch"  checked="true"/>
 </view>

  <button bindtap="btnSave" class="save_btn">发表</button>
</view>
           